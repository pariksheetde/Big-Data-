import numpy as np
import pandas as pd
import matplotlib.pyplot as plt
%matplotlib inline

data = pd.read_csv("D:\Code\Python\DataSet\Tech_Data.csv")
age = data["Age"]
dev = data["All_Devs"]
py = data["Python"]
js = data["JavaScript"]

plt.plot(age, dev, color = "#444444", label = "Developer's Salary")
plt.plot(age, py, color = "black", label = "Python's Salary")
plt.plot(age, js, color = "cyan", label = "Java's Salary")

plt.title("Median Salary (USD) by Age")
plt.fill_between(age,
                 py,
                 y2 = dev,
                 where = (py > dev),
                 interpolate = True,
                 alpha = 0.25,
                 color = "cyan"
                 )

plt.fill_between(age,
                 py,
                 y2 = dev,
                 where = (py <= dev),
                 interpolate = True,
                 alpha = 0.25,
                 color = "green"
                 )

plt.legend(loc = (0.07,0.05))
plt.grid()
plt.tight_layout()
plt.style.use("ggplot")
fig = plt.gcf()
fig.set_size_inches(11,8)
plt.show()

