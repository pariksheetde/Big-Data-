------------------------------------------------------------------------CLONE TABLE-------------------------------------------------------------------------------------------
CREATE TABLE employees_cp CLONE employees;

------------------------------------------------------------------------CLONE SCHEMA------------------------------------------------------------------------------------------
CREATE SCHEMA HR_cp CLONE HR;

TIME TRAVEL
SELECT * FROM customers before (timestamp => '2020-01-01 15:26:31.855' :: timestamp)

------------------------------------------------------------------------TIME TRAVEL-------------------------------------------------------------------------------------------
SELECT * FROM customers AT (offset => -60 *20)

