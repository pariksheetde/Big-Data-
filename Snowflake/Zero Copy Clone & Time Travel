SELECT * FROM customer before(statement => [ENTER THE QUERY_ID])

CLONE TABLE WITH TIME TRAVEL 
CREATE TABLE employees_cp CLONE employees before (timestamp => '2020-02-03 12:21:53.654' ::timestamp);

